{
  "rewrites": [
    {
      "source": "/umami/script.js",
      "destination": "https://analytics.teogoulois.com/script.js"
    },
    {
      "source": "/api/send",
      "destination": "https://analytics.teogoulois.com/script.js/api/send"
    },
    {
      "source": "/api/collect",
      "destination": "https://analytics.teogoulois.com/script.js/api/collect"
    }
  ],
  "headers": [
    {
      "source": "/umami/script.js",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}
